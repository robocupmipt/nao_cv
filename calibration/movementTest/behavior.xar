	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("Back")):
			self.output_1(p)
		elif(p == self.typeConversion("Knee")):
			self.output_2(p)
		elif(p == self.typeConversion("Frog")):
			self.output_3(p)
		elif(p == self.typeConversion("Belly")):
			self.output_4(p)
		else:
			self.onDefault()

