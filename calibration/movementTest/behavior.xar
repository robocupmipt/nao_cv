        self.motion = ALProxy("ALMotion")
        footGaitConfigMin = self.motion.getFootGaitConfig("Min")
        footGaitConfigMax = self.motion.getFootGaitConfig("Max")
        self.dFootGaitConfig = {}
        for i in range(len(footGaitConfigMin)):
