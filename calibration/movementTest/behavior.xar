        if (flag == 1):
            global imNum
            imNum = imNum+1
            self.detectionGood()
            self.onUnload()
            #self.onStopped()
        else:
            self.detectionFailure()
            self.onUnload()
            #self.onStopped() #~ activate output of the box
        pass

    def onInput_onStop(self):
